{
  "_meta": {
    "sources": [
      "loopback/common/models",
      "loopback/server/models",
      "../common/models",
      "./models"
    ],
    "mixins": [
      "loopback/common/mixins",
      "loopback/server/mixins",
      "../common/mixins",
      "./mixins"
    ]
  },
  "Customer": {
    "dataSource": "postgresql",
    "public": true,
    "options": {
      "remoting": {
        "sharedMethods": {
          "*": false,
          "register": true,
          "login": true,
          "logout": true,
          "profile": true,
          "updateDetails": true,
          "getAllCustomers": true
        }
      }
    }
  },
  "MenuItem": {
    "dataSource": "postgresql",
    "public": true,
    "options": {
      "remoting": {
        "sharedMethods": {
          "*": false,
          "getAllMenuitems": true,
          "createMenuItem": true
        }
      }
    }
  },
  "Order": {
    "dataSource": "postgresql",
    "public": true,
    "options": {
      "remoting": {
        "sharedMethods": {
          "*": false,
          "createOrder": true
        }
      }
    }
  }
}